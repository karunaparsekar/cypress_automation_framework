{
  "name": "cypress_automation_framework",
  "version": "1.0.0",
  "description": "Cypress Automation Framework",
  "main": "index.js",
  "scripts": {
    "triggerAllTests-headless": "npx cypress run",
    "triggerAllTests-headed": "npx cypress run --headed",
    "triggerAllTests-MainMenu": "npx cypress run --spec cypress/e2e/features/homeFeatures/MainMenu.feature",
    "triggerAllTests-Login": "npx cypress run --spec cypress/e2e/features/homeFeatures/Login.feature",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Karuna Parsekar",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^13.17.0",
    "@badeball/cypress-cucumber-preprocessor":  "^4.3.1",
    "mochawesome": "^6.1.1",
    "mochawesome-merge": "^4.1.0",
    "mochawesome-report-generator": "^5.1.0",
    "cypress-multi-reporters": "^1.4.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "stepDefinitions": "cypress/support/step_definitions/**/*.{js,ts}",
    "compiler": "js:babel-register"
  },
  "type": "module"
}